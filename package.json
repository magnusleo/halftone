{
  "name": "halftone",
  "version": "1.0.0",
  "description": "Halftone",
  "main": "src/index.js",
  "devDependencies": {
    "prettier": "1.14.3",
    "ts-loader": "^5.2.1",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
